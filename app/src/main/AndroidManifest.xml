<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lizhuo.kotlinlearning">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.wq.common.base.App"
        android:allowBackup="true"
        android:icon="@drawable/avter"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.wq.notebook.home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wq.notebook.home.Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme" />
        <activity android:name="com.wq.notebook.home.NoteListActivity" />
        <activity android:name="com.wq.notebook.home.AddNoteActivity"></activity>
    </application>

</manifest>